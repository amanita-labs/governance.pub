# Frontend Environment Variables
# Copy this file to .env.local for local development

# Backend API URL (Required)
# This should point to your deployed backend service on Render
# For local development, use: http://localhost:8080
# For production, use: https://your-backend.onrender.com
NEXT_PUBLIC_BACKEND_URL=http://localhost:8080

# Cardano Network (Optional)
# Used for client-side components (e.g., explorer URLs)
# Values: mainnet or preview
# Default: mainnet
NEXT_PUBLIC_NETWORK=preview


NEXT_PUBLIC_ENABLE_DASHBOARD=false
